				type = "passive";
			};
		};
	};

	pa-therm2-usr {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm8150l_adc_tm ADC_AMUX_THM1_PU2>;
		wake-capable-sensor;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	mem-therm-usr {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm8150l_adc_tm ADC_AMUX_THM2_PU2>;
		wake-capable-sensor;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	sdm-therm-usr {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm8150l_adc_tm ADC_AMUX_THM3_PU2>;
		wake-capable-sensor;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	front-usr {
		polling-delay-passive = <2000>;
		polling-delay = <2000>;
		thermal-governor = "user_space";
	};

	back-usr {
		polling-delay-passive = <2000>;
		polling-delay = <2000>;
		thermal-governor = "user_space";
	};

	front-step {
		polling-delay-passive = <2000>;
		polling-delay = <2000>;
		thermal-governor = "step_wise";
		trips {
			modem_trip1: modem-trip1 {
				temperature = <46000>;
				hysteresis = <2000>;
				type = "passive";
			};
			modem_trip2: modem-trip2 {
				temperature = <50000>;
				hysteresis = <3000>;
				type = "passive";
			};
		};
		cooling-maps {
			/* Throttle DL speeds */
			skin_modem_lvl1 {
				trip = <&modem_trip1>;
				cooling-device = <&modem_pa_fr1 1 1>;
			};
			/* Disable 5G */
			skin_modem_lvl2 {
				trip = <&modem_trip2>;
				cooling-device = <&modem_pa_fr1 2 2>;
			};
		};
	};
};

