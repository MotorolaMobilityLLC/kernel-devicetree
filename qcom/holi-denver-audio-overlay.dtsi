&qupv3_se10_i2c {
        status = "ok";
        aw882xx: aw882xx_smartpa@34 {
			compatible = "awinic,aw882xx_smartpa";
			reg = <0x34>;
			pinctrl-names = "default";
			pinctrl-0 = <&aw882xx_irq_default>, <&aw882xx_reset_default>;
			reset-gpio = <&tlmm 58 0>;
			irq-gpio = <&tlmm 59 0x2008>;
			monitor-flag = <1>;
			monitor-timer-val = <3000>;
			afe-rx-portid = <0x1002>;
			afe-tx-portid = <0x1003>;
			low-vol-table-down = <3650 9 3 3750 9 2 3900 9 1>;
			low-vol-table-up = <4000 9 0 3800 9 1 3700 9 2>;
			low-temp-table-down = <0xFFFFFFFB 0x4 0x10 0xFFBEAE7E 0x0 0x5 0x9 0xFFD80505 0x5 0x6 0x6 0xFFED714D>;
			low-temp-table-up = <0x7 0x9 0x0 0x00000000 0x2 0x6 0x06 0xFFED714D 0xFFFFFFE 0x05 0x09 0xFFD80505>;
			status = "okay";
		};
		
	wsa881x_i2c_e: wsa881x-i2c-codec@e {
		status = "disabled";	
		compatible = "qcom,wsa881x-i2c-codec";
		reg = <0x0e>;
		clock-names = "wsa_mclk";
		clocks = <&wsa881x_analog_clk 0>;
		qcom,wsa-analog-clk-gpio = <&wsa881x_analog_clk_gpio>;
		qcom,wsa-analog-reset-gpio = <&wsa881x_analog_reset_gpio>;
		cdc-vdd-1p8-supply = <&L11A>;
		qcom,cdc-vdd-1p8-voltage = <1800000 1800000>;
		qcom,cdc-vdd-1p8-current = <10000>;
		qcom,cdc-static-supplies = "cdc-vdd-1p8";
		qcom,wsa-prefix = "SpkrMono";
	};

	wsa881x_i2c_44: wsa881x-i2c-codec@44 {
		status = "disabled";	
		compatible = "qcom,wsa881x-i2c-codec";
		reg = <0x044>;
	};

	wsa881x_i2c_f: wsa881x-i2c-codec@f {
		status = "disabled";
		compatible = "qcom,wsa881x-i2c-codec";
		reg = <0x0f>;
		clock-names = "wsa_mclk";
		clocks = <&wsa881x_analog_clk 0>;
		qcom,wsa-analog-clk-gpio = <&wsa881x_analog_clk_gpio>;
		qcom,wsa-analog-reset-gpio = <&wsa881x_analog_reset_gpio>;
		cdc-vdd-1p8-supply = <&L11A>;
		qcom,cdc-vdd-1p8-voltage = <1800000 1800000>;
		qcom,cdc-vdd-1p8-current = <10000>;
		qcom,cdc-static-supplies = "cdc-vdd-1p8";
		qcom,wsa-prefix = "SpkrMono";
	};

	wsa881x_i2c_45: wsa881x-i2c-codec@45 {
		status = "disabled";
		compatible = "qcom,wsa881x-i2c-codec";
		reg = <0x045>;
	};
		
};

