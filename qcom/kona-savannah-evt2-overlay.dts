/dts-v1/;
/plugin/;

#include "kona-moto-common-overlay.dtsi"

/ {
	model = "Savannah";
	compatible = "qcom,kona-savannah", "qcom,kona-moto", "qcom,kona";
	qcom,msm-id = <356 0x20000>;
	qcom,board-id = <0 0xA200>;
};

/*
 * Savannah HW rev EVT2 specific changes go here
 */

&qupv3_se13_i2c {
	goodix_ts_i2c@5d {
		goodix,x2x;
		goodix,cfg_bin_name = "goodix_cfg_group_smd.bin";
	};
};

&rear_pmic {
	status = "disabled";
};

&main_sensor {
	csiphy-sd-index = <2>;
	pmicreset-gpios = <&pm8150l_gpios 5 0>;
	slg,pmic-workaround;
};

&front_sensor {
	csiphy-sd-index = <1>;
	cci-master = <0>;
};

&wide_sensor {
	csiphy-sd-index = <0>;
	cci-master = <1>;
	pmicreset-gpios = <&pm8150l_gpios 5 0>;
	slg,pmic-workaround;
};

&tele_sensor {
	csiphy-sd-index = <3>;
	pmicreset-gpios = <&pm8150l_gpios 5 0>;
	slg,pmic-workaround;
};

&cs35l41_rcv {
	reset-gpios = <&tlmm 139 0>;
	interrupt-parent = <&tlmm>;
	interrupts = <138 IRQ_TYPE_LEVEL_LOW>;
	interrupt-names = "prince_irq";
	/delete-property/  pinctrl-0;
	/delete-property/  pinctrl-names;
};
