        wt6670_pins_default: wt6670_default_mode {
        };
        wt6670_pins_scl_low: wt6670_scl_low {
		mux {
			pins = "gpio1";
			function = "gpio";
		};

		config {
			pins = "gpio1";
			drive-strength = <2>;
			bias-pull-down;
		};
        };
        wt6670_pins_scl_high: wt6670_scl_high {
		mux {
			pins = "gpio1";
			function = "gpio";
		};

		config {
			pins = "gpio1";
			drive-strength = <2>;
			bias-pull-up;
			output-high;
		};
        };
        wt6670_pins_sda_low: wt6670_sda_low {
		mux {
			pins = "gpio0";
			function = "gpio";
