&tlmm{
	qupv3_se0_i2c_pins: qupv3_se0_i2c_pins {
		qupv3_se0_i2c_act: qupv3_se0_i2c_active {
			mux {
				pins = "gpio0", "gpio1";
				function = "qup0";
			};

			config {
				pins = "gpio0", "gpio1";
				drive-strength = <2>;
				bias-disable;
			};
		};

		qupv3_se0_i2c_sle: qupv3_se0_i2c_sleep {
			mux {
				pins = "gpio0", "gpio1";
				function = "gpio";
			};

			config {
				pins = "gpio0", "gpio1";
				drive-strength = <2>;
				bias-pull-up;
				input-enable;
			};
		};
	};
};

