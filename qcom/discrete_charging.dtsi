		};
        };
        wt6670_pins_sda_high: wt6670_sda_high {
		mux {
			pins = "gpio0";
			function = "gpio";
		};

		config {
			pins = "gpio0";
			drive-strength = <2>;
			bias-pull-up;
			output-high;
		};
        };
