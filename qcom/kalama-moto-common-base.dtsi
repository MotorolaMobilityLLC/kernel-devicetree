/* This file is derivated from kalama.dts */

/dts-v1/;

#include "kalama.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. Kalama SoC";
	compatible = "qcom,kalama";
	qcom,board-id = <0 0>;

/*
 * Moto common base dtb nodes definition
 */
	channel-id-map = "";
	chosen: chosen { };
	aliases: aliases { };
	reserved_memory: reserved-memory { };
	mot_batterydata: qcom,battery-data { };
	utags: utags { };
	hw: hw { };
};

&reserved_memory {
	/delete-node/ ramoops_region;
};

&firmware {
	android {
		/delete-node/ fstab;
	};
};

/delete-node/ &mem_offline;

&apps_smmu {
	/delete-property/ dma-coherent;
};
