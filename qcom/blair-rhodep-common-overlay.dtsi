		Semtech,ref-phases-b = <6>;
		Semtech,ref-phases-c = <0xff>;
		Semtech,button-flag = <0x1f>;
		Semtech,reg-num = <75>;
		Semtech,reg-init = <
			0x8024  0x7F00
			0x4004  0x70
			0x8008  0x0
			0x4280  0x0
			0x8020  0x32
			0x8028  0x886 		/*AFE_PARAM_PH0*/
