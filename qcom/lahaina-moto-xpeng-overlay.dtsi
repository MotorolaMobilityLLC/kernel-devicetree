&qupv3_se13_i2c {
	status = "ok";
	nq@64 {
		status = "disabled";
	};

	tps65132@3e {
		compatible = "ti,tps65132";
		reg = <0x3e>;

		regulators {
			tps65132_outp: outp {
				regulator-name = "outp";
				regulator-boot-on;
				regulator-always-on;
				enable-gpios = <&tlmm 2 0>;
			};

			tps65132_outn: outn {
				regulator-name = "outn";
				regulator-boot-on;
				regulator-always-on;
				regulator-active-discharge = <0>;
				enable-gpios = <&tlmm 3 0>;
			};
		};
	};

        aw99703@36 {
                compatible = "awinic,aw99703-bl";
                reg = <0x36>;

                aw99703,hwen-gpio = <&tlmm 13 0>;
                aw99703,bl-map = <3>; /* brightness map level 1:256,2:1024,3:2048 */
                aw99703,using-lsb;
                aw99703,ovp-level = <2>; /* 31V */
                aw99703,ocp-level = <1>; /* 1.8A */
                aw99703,switch-freq = <1>; /* 1000kHz(default) */
                aw99703,auto-freq-high = <0x10>;
                aw99703,auto-freq-low = <0x05>;
                aw99703,bl-fscal-led = <0x19>; /* 24.8mA */
                aw99703,bl-channel = <3>;
                aw99703,turn-on-ramp = <0x09>; /* 256ms */
                aw99703,turn-off-ramp = <0x04>; /* 8ms */
                aw99703,pwm-trans-dim = <0>; /* 2ms */
                aw99703,i2c-trans-dim = <0x07>; /* 512ms */
                aw99703,pwm-mode = <1>; /* 1 pwm enable,0 PWM disable */
                aw99703,map-type = <1>; /* linear */
                aw99703,work-mode = <1>; /* backlight */
	};
};

&tlmm {
	redriver_enable: redriver_enable {
		mux {
			pins = "gpio27";
			function = "gpio";
		};

		config {
			pins = "gpio27";
			drive-strength = <2>;
			bias-disable; /* NO PULL */
			input-enable;
		};
	};
	redriver_disable: redriver_disable {
		mux {
			pins = "gpio27";
				function = "gpio";
			};

		config {
			pins = "gpio27";
			drive-strength = <2>;
			output-low;
		};
	};

};

