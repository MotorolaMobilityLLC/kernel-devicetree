#include "waipio-moto-common-overlay-base.dtsi"
#include "waipio-moto-common-overlay.dtsi"
#include "waipio-moto-common-charger.dtsi"

&qupv3_se15_i2c {
	status = "ok";

	redriver: redriver@1A {
		compatible = "onnn,redriver";
		reg = <0x1A>;
		vcc-supply = <&S10B>;
		pinctrl-names = "default";
		pinctrl-0 = <&redriver_enable>;
		gpios = <&tlmm 109 0>;

		eq = /bits/ 8 <
			/* Parameters for USB */
			0x4 0x4 0x4 0x4
			/* Parameters for DP */
			0x6 0x4 0x4 0x6>;
		flat-gain = /bits/ 8 <
			/* Parameters for USB */
			0x3 0x1 0x1 0x3
			/* Parameters for DP */
			0x2 0x1 0x1 0x2>;
		output-comp = /bits/ 8 <
			/* Parameters for USB */
			0x3 0x3 0x3 0x3
			/* Parameters for DP */
			0x3 0x3 0x3 0x3>;
		loss-match = /bits/ 8 <
			/* Parameters for USB */
			0x1 0x3 0x3 0x1
			/* Parameters for DP */
			0x3 0x3 0x3 0x3>;
	};

	fusb0: typec-portc@22 {
		compatible = "fcs,fusb302";
		reg = <0x22>;
		interrupt-parent = <&tlmm>;
		interrupts = <85 IRQ_TYPE_LEVEL_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&fusb0_int>;
		vbus-supply = <&BOB>;
		status = "ok";
	};

	pct1812_mmi@33 {
		compatible = "pixart,pct1812_ts";
		reg = <0x33>;
		/*pct1812,reset-gpio = <&tlmm 20 0>;*/
		pct1812,irq-gpio = <&tlmm 84 0x2008>;
		pct1812,regulator_vdd = "pm8350c_l3";
		pct1812,regulator_vio = "pm8350_s10";
		pinctrl-names = "touchpad_active", "touchpad_suspend";
		pinctrl-0 = <&tpad_int_active>;
		pinctrl-1 = <&tpad_int_active>;
	};
};

&usb0 {
	ssusb_redriver = <&redriver>;
};

&redriver {
	status = "ok";
};

&pm8350c_rgb {
	red {
	        label = "charging";
	};

	green {
	        status = "disabled";
	};

	blue {
	        status = "disabled";
	};
};

&tlmm {
	redriver_enable: redriver_enable {
		mux {
			pins = "gpio109";
			function = "gpio";
		};

		config {
			pins = "gpio109";
			drive-strength = <2>;
			bias-disable; /* NO PULL */
			output-high;
		};
	};

	redriver_disable: redriver_disable {
		mux {
			pins = "gpio109";
			function = "gpio";
		};

		config {
			pins = "gpio109";
			drive-strength = <2>;
			output-low;
		};
	};

	flip_balance_en_n: flip-balance-en-n {
		mux {
			pins = "gpio105";
			function = "gpio";
		};

		config {
			pins = "gpio105";
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};

	flip_battplus_dis: flip-battplus-dis {
		mux {
			pins = "gpio90";
			function = "gpio";
		};

		config {
			pins = "gpio90";
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};

	fusb0_int: fusb0-int {
		mux {
			pins = "gpio85";
			function = "gpio";
		};

		config {
			pins = "gpio85";
			drive-strength = <2>;
			bias-pull-up;
		};
	};

	tpad_int_active: tpad-int-active {
		mux {
			pins = "gpio84";
			function = "gpio";
		};

		config {
			pins = "gpio84";
			drive-strength = <8>;
			bias-pull-up;
		};
	};
};

&pm8350_gpios {
	/delete-node/ pm8350_rear_tof_therm;

	pm8350_wifi_skin_therm {
		pm8350_wifi_skin_therm_default: pm8350_wifi_skin_therm_default {
			pins = "gpio1";
			bias-high-impedance;
		};
	};
};

&pm8350b_gpios {
	pm8350b_chg_therm {
		pm8350b_chg_therm_default: pm8350b_chg_therm_default {
			pins = "gpio4";
			bias-high-impedance;
		};
	};

	chrg_fullcurr_en {
		chrg_fullcurr_en_default: chrg_fullcurr_en_default {
			pins = "gpio8";
			function = "normal";
			output-high;
			input-disable;
			bias-disable;
			power-source = <0>;       /* 1.8V */
			qcom,drive-strength = <2>;
		};
	};
};

&soc {
	flip_fet_control {
		compatible = "mmi,fet-control";
		pinctrl-names = "default";
		pinctrl-0 = <&flip_battplus_dis &flip_balance_en_n &chrg_fullcurr_en_default>;
		mmi,main-batt-psy = "battery";
		mmi,flip-batt-psy = "flip_battery";
		mmi,flip_battplus_en_gpio = <&tlmm 90 0>;
		mmi,flip_balance_en_n_gpio = <&tlmm 105 0>;
		mmi,chrg-fullcurr-en-gpio = <&pm8350b_gpios 8 GPIO_ACTIVE_HIGH>;
	};
};

&qti_glink_charger {
			mmi,max-fv-mv = <4490>;
			mmi,max-fcc-ma = <3000>;
			mmi,demo-fv-mv = <3000>;
};



