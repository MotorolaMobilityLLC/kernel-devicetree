			};
			config {
				pins = "gpio129";
				drive-strength = <2>;   /* 2 mA */
				bias-pull-down;
			};
		};
	};

	p938x_outputs: p938x-outputs {
		mux {
			pins = "gpio107", "gpio82", "gpio131";
			function = "gpio";
		};

		config {
			pins = "gpio107", "gpio82", "gpio131";
			drive-strength = <2>;
			bias-pull-up;
			output-low;
		};
	};

	p938x_inputs: p938x-inputs {
		mux {
