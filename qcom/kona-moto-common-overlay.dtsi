			function = "gpio";
		};

		config {
			pins = "gpio108", "gpio83";
			drive-strength = <2>;
			bias-pull-up;
			input-enable;
		};
	};

	capsense_gpio_pins: capsense_gpio_pins {
		mux {
			pins = "gpio64";
			function = "gpio";
		};
		config {
			pins = "gpio64";
			drive-strength = <2>;
			bias-disable;
		};
	};
};

