&tlmm{
        bq25960_int_default: bq25960_int_default {
                mux {
                        pins = "gpio11";
                        function = "gpio";
                };

                config {
                        pins = "gpio11";
                        drive-strength = <2>; /* 2 MA */
                        bias-pull-up;
		//	bias_disable;
                        input-enable;
                };
        };
};

&qupv3_se16_i2c {
	status = "okay";
	qcom,clk-freq-out = <400000>;
	bq25960@65 {
		compatible = "ti,bq25960-standalone";
		reg = <0x65>;
		/delete-property/ interrupts;
                interrupt-parent = <&tlmm>;
                interrupts = <11 0>;
                interrupt-names = "bq25960_irq";
                pinctrl-names = "default";
                pinctrl-0 = <&bq25960_int_default>;
	};
};

&pm8150b_charger {
        status = "ok";
//        qcom,sec-charger-config = <0>;
        qcom,auto-recharge-soc = <99>;
        io-channels = <&pm8150b_vadc ADC_MID_CHG_DIV6>,
                      <&pm8150b_vadc ADC_USB_IN_I>,
                      <&pm8150b_vadc ADC_SBUx>,
                      <&pm8150b_vadc ADC_VPH_PWR>,
                      <&pm8150b_vadc ADC_CHG_TEMP>;
        io-channel-names = "mid_voltage",
                           "usb_in_current",
                           "sbux_res",
                           "vph_voltage",
                           "chg_temp";
        qcom,battery-data = <&mot_batterydata>;
        qcom,fcc-max-ua = <4000000>;
        qcom,fv-max-uv = <4480000>;
        qcom,usb-icl-ua = <3000000>;
        qcom,wd-bark-time-secs = <16>;
        qcom,suspend-input-on-debug-batt;
        qcom,usbpd-phandle = <&pm8150b_pdphy>;
        qcom,typec-legacy-rp-icl;
        qcom,disable-suspend-on-collapse;
	qcom,dc-unsupported;
	qcom,wireless-unsupported;
	qcom,usbeb-unsupported;
	qcom,afvc_enable;
	qcom,lpd-disable;
	qcom,thermal-mitigation = <4000000 3500000
				3000000 2500000
				2000000 1500000
				1200000 900000
				600000  400000>;
};

&spmi_bus {
        qcom,pm8150b@2 {
                pm8150b_mmi: qcom,qpnp-smb5-mmi {
                        compatible = "qcom,qpnp-smbcharger-mmi";
                        qcom,pmic-revid = <&pm8150b_revid>;
                        vbus-supply = <&smb5_vbus>;
                        status = "ok";
                        qcom,enable-charging-limit;
                        qcom,upper-limit-capacity = <65>;
                        qcom,lower-limit-capacity = <60>;
                        qcom,mmi-temp-zones =   <0  4200 873 0>,
                                                <10 4250 2180 873>,
                                                <15 0 2180 2180>,
						<23 0 4000 4000>,
						<35 0 4000 4000>,
                                                <45 0 4000 4000>,
                                                <60 4200 2180 0>;
                	qcom,mmi-ffc-zones = <4450 218 (-500)>,
                                        <4450 218 (-500)>,
                                        <4450 218 (-500)>,
                                        <4480 510 (-710)>,
                                        <4480 620 (-820)>,
                                        <4480 680 (-880)>,
                                        <4450 218 (-500)>;
                        qcom,iterm-ma = <218>;
                        qcom,vfloat-comp-uv = <20000>;
                        qcom,hvdcp-power-max = <20000>;
                };
        };

        qcom,pm8150@0 {
                qcom,power-on-mmi@800 {
                        compatible = "qcom,qpnp-power-on-mmi";
                        interrupts = <0x0 0x8 0x3 IRQ_TYPE_NONE>;
                        interrupt-names = "kpdpwr-bark";
                };
        };
};

&pm8150b_fg {
        status = "ok";
        qcom,battery-data = <&mot_batterydata>;
        qcom,hold-soc-while-full;
        qcom,linearize-soc;
        qcom,cl-wt-enable;
        qcom,fg-force-load-profile;
        /* ESR fast calibration */
        qcom,fg-esr-timer-chg-fast = <0 7>;
        qcom,fg-esr-timer-dischg-fast = <0 7>;
        qcom,fg-esr-timer-chg-slow = <0 96>;
        qcom,fg-esr-timer-dischg-slow = <0 96>;
        qcom,fg-esr-cal-soc-thresh = <26 230>;
        qcom,fg-esr-cal-temp-thresh = <10 40>;
        qcom,fg-cutoff-voltage = <3200>;
        qcom,fg-empty-voltage = <3000>;
        qcom,fg-sys-min-voltage = <3000>;
        qcom,fg-sys-term-current = <(-400)>;
        qcom,ki-coeff-low-chg = <1500>;
        qcom,ki-coeff-med-chg = <1500>;
        qcom,ki-coeff-hi-chg = <123>;
	/delete-property/qcom,rbat-conn-mohm;
	/*mmi,cp-use-internal-qg;*/
};

