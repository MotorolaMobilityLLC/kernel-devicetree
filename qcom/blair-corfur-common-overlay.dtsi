		polling-delay = <20000>;
		thermal-governor = "user_space";
		thermal-sensors = <&pmk8350_adc_tm PMK8350_ADC7_AMUX_THM1_100K_PU>;
		wake-capable-sensor;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};
};

&pm8008_8 {
	status = "disabled";
};

&pm8008_9 {
	status = "disabled";
};

&sdhc_2 {
	qcom,vdd-voltage-level = <3000000 3000000>;
};

&qupv3_se10_i2c_sleep {
	config {
		/delete-property/ bias-disable;
		bias-pull-up;
		input-enable;
	};
};


&pm7250b_charger {
	dpdm-supply = <&usb2_phy0>;
};

&usb2_phy0 {
	qcom,param-override-seq =
		<0x61 0x6c	/* override_x0 */
		0x85 0x70	/* override_x1 */
		0x2e 0x74>;	/* override x2 */
};
